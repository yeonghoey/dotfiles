#!/bin/bash

if [[ "$(uname)" == "Linux" ]]; then
  dropbox.py start
fi

echo 'Pull yeonghoey'
git -C "$HOME/repos/yeonghoey" pull
echo

echo 'Pull notes'
git -C "$HOME/repos/notes" pull
echo

echo 'Pull .spacemacs.d'
git -C "$HOME/.spacemacs.d" pull
echo

echo 'Pull .dotfiles'
git -C "$HOME/.dotfiles" pull
echo

emacs &
